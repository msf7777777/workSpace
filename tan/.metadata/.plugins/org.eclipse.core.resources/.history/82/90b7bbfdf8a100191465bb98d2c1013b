package Diary0622;

public class day0622_02_for_QianTao {
	public static void main(String[] args) {
		for(int x = 0,y = 9;x <=5 && y >= 5;x++,y--){
			System.out.print(x);
			System.out.print(y);
			System.out.println();
		}
		System.out.println("**********");
		
		
		//for循环的嵌套
		//每个联系都有1-3题，做四个练习
		//练习1的第1题
		//练习1的第2题
		//练习1的第3题
		//练习2的第1题
		//练习2的第2题
		//练习2的第3题
		//练习3的第1题
		//练习3的第2题
		//练习3的第3题
		
		for(int a = 1;a <= 3;a++){
			System.out.println(a);
		}
		for(int b = 1;b <= 3;b++){
			System.out.println(b);
		}
		for(int c = 1;c <= 3;c++){
			System.out.println(c);
		}
		
		
		
		for(int x = 1;x <= 3;x++){
			System.out.println("做练习" + x);
			for(int y = 1;y <= 3;y++){
				System.out.println("第" + y + "题");
			}
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
	}
}
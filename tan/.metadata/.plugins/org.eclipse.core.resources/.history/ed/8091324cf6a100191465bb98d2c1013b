package Diary0622;

public class day0622_02_for_QianTao {
	public static void main(String[] args) {
		for(int x = 0,y = 9;x <=5 && y >= 5;x++,y--){
			System.out.print(x);
			System.out.print(y);
			System.out.println();
		}
		
		//for循环的嵌套
		//每个联系都有1-3题，做四个练习
		//练习1的第1题
		//练习1的第2题
		//练习1的第3题
		
		
		
		
		
		
	}
}
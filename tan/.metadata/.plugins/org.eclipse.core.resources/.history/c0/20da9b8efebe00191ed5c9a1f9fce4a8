package No13_接口2;

import java.util.Scanner;

public class day0814_01 {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		
		System.out.println("№ 输入_a №");
		String n = s.next();
		
		System.out.println("№ 输入_b №");
		int a = s.nextInt();
		
		System.out.println("№ 输入_c №");
		String g = s.next();
		
		System.out.println("№ 输入_b №");
		int sno = s.nextInt();	
		
		s.close();
		Person p1 = new Person(n,a,g,sno);
	}	
}

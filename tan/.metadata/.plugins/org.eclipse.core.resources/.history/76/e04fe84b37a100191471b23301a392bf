package diary0620;

public class day0620_05_if_else {
	public static void main(String[] args) {
		//没有块的if
		int number = 5;
		if(number < 0){
			number = number + 10;
			number++;
		}
		System.out.println("正能量");
		
		if(number > 0){
//			number = number + 10;
//			number++;
		}
		System.out.println("负能量");
		
		
		
		
		
		
	}
}

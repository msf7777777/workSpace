package No13_接口2;

import java.util.Scanner;

public class day0814_01 {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		
		System.out.println("№ 输入_姓名 №");
		String name = s.next();
		
		System.out.println("№ 输入_年龄 №");
		int age = s.nextInt();
		
		System.out.println("№ 输入_性别 №");
		String gender = s.next();
		
		System.out.println("№ 输入_学号 №");
		int sno = s.nextInt();	
		
		s.close();
		
		Person p1 = new Person("麻少飞",29,"男",19901113);
		Person p1 = new Person(name,age,gender,sno);
	}	
}
